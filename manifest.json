{
  "manifest_version": 3,
  "name": "Phishing URL Detector",
  "version": "1.0",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "host_permissions": ["http://localhost:5000/*"]
}
